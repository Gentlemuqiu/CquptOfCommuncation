\"mini\" @click=\"handleEdit(scope.row)\">编辑</el-button>\n          <el-popconfirm title=\"确定删除吗？\" @confirm=\"handleDelete(scope.row.id)\">\n            <template #reference>\n              <el-button size=\"mini\" type=\"danger\">删除</el-button>\n            </template>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <div style=\"margin: 10px 0\">\n      <el-pagination\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n          :current-page=\"pageNum\"\n          :page-sizes=\"[5, 10, 20]\"\n          :page-size=\"pageSize\"\n          layout=\"total, sizes, prev, pager, next, jumper\"\n          :total=\"total\">\n      </el-pagination>\n    </div>\n\n    <el-dialog title=\"信息\" v-model=\"dialogVisible\" width=\"30%\">\n    <el-form :model=\"entity\" label-width=\"120px\">\n        <el-form-item label=\"名称\">\n          <el-input v-model=\"entity.name\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"图片\">\n          <el-upload action=\"http://localhost:9090/files/upload\" :on-success=\"fileSuccessUpload\" :file-list=\"fileList\">\n            <el-image style=\"width: 100px\" :src=\"entity.img\"></el-image>\n<!--            <el-button size=\"small\" type=\"primary\">点击上传</el-button>-->\n          </el-upload>\n        </el-form-item>\n      <el-form-item label=\"上传视频\">\n        <el-upload action=\"http://localhost:9090/files/upload\" :on-success=\"movieSuccessUpload\" :file-list=\"fileList\">\n          <el-button size=\"small\" type=\"primary\">点击上传</el-button>\n        </el-upload>\n      </el-form-item>\n\n    </el-form>\n      <template #footer>\n          <span class=\"dialog-footer\">\n            <el-button @click=\"dialogVisible = false\">取 消</el-button>\n            <el-button type=\"primary\" @click=\"save\">确 定</el-button>\n          </span>\n      </template>\n    </el-dialog>\n\n  </div>\n</template>\n\n<script>\n\nimport request from \"@/utils/request\";\nlet url = \"/movie\"\n\nexport default {\n  name: 'Movie',\n  components: {\n\n  },\n  data() {\n    return {\n      loading: true,\n      entity: {},\n      dialogVisible: false,\n      search: '',\n      pageNum: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: [],\n      fileList: []\n    }\n  },\n  created() {\n    this.load()\n  },\n  methods: {\n    download(url) {\n      window.open(url)\n    },\n    fileSuccessUpload(res) {\n      this.entity.img = res.data;\n    },\n    movieSuccessUpload(res) {\n      this.entity.url = res.data;\n    },\n    load() {\n      this.loading = true\n      request.get(url + \"/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        this.loading = false\n        this.tableData = res.data.records\n        this.total = res.data.total\n      })\n    },\n    add() {\n      this.dialogVisible = true\n      this.entity = {}\n      this.fileList = []\n    },\n    save() {\n      if (this.entity.id) {  // 更新\n        request.put(url, this.entity).then(res => {\n          console.log(res)\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            })\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            })\n          }\n          this.load() // 刷新表格的数据\n          this.dialogVisible = false  // 关闭弹窗\n        })\n      }  else {  // 新增\n        request.post(url, this.entity).then(res => {\n          console.log(res)\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            })\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            })\n          }\n\n          this.load() // 刷新表格的数据\n          this.dialogVisible = false  // 关闭弹窗\n        })\n      }\n\n    },\n    handleEdit(row) {\n      this.entity = JSON.parse(JSON.stringify(row))\n      this.dialogVisible = true\n      this.fileList = []\n    },\n    handleDelete(id) {\n      request.delete(url + \"/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          })\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          })\n        }\n        this.load()  // 删除之后重新加载表格的数据\n      })\n    },\n    handleSizeChange(pageSize) {   // 改变当前每页的个数触发\n      this.pageSize = pageSize\n      this.load()\n    },\n    handleCurrentChange(pageNum) {  // 改变当前页码触发\n      this.pageNum = pageNum\n      this.load()\n    }\n  }\n}\n</script>\n"],"mappings":";;EACOA,KAAqB,EAArB;IAAA;EAAA;AAAqB;;EAGnBA,KAAsB,EAAtB;IAAA;EAAA;AAAsB;;EAKtBA,KAAsB,EAAtB;IAAA;EAAA;AAAsB;;EAiCtBA,KAAsB,EAAtB;IAAA;EAAA;AAAsB;;EA+BfC,KAAK,EAAC;AAAe;;;;;;;;;;;;;;uBAxEnCC,mBAAA,CA+EM,OA/ENC,UA+EM,GA7ERC,mBAAA,YAAe,EACXC,mBAAA,CAEM,OAFNC,UAEM,GADJC,YAAA,CAAqDC,oBAAA;IAA1CC,IAAI,EAAC,SAAS;IAAEC,OAAK,EAAEC,QAAA,CAAAC;;sBAAK,MAAEC,MAAA,QAAAA,MAAA,O,iBAAF,IAAE,E;;oCAG/CT,mBAAA,YAAe,EACXC,mBAAA,CAGM,OAHNS,UAGM,GAFJP,YAAA,CAAwFQ,mBAAA;gBAArEC,KAAA,CAAAC,MAAM;+DAAND,KAAA,CAAAC,MAAM,GAAAC,MAAA;IAAEC,WAAW,EAAC,QAAQ;IAACnB,KAAkB,EAAlB;MAAA;IAAA,CAAkB;IAACoB,SAAS,EAAT;2CACnEb,YAAA,CAA+EC,oBAAA;IAApEC,IAAI,EAAC,SAAS;IAACT,KAAwB,EAAxB;MAAA;IAAA,CAAwB;IAAEU,OAAK,EAAEC,QAAA,CAAAU;;sBAAM,MAAER,MAAA,QAAAA,MAAA,O,iBAAF,IAAE,E;;mEAErES,YAAA,CA2BWC,mBAAA;IAzBNC,IAAI,EAAER,KAAA,CAAAS,SAAS;IAChBC,MAAM,EAAN,EAAM;IACNC,MAAM,EAAN,EAAM;IACN3B,KAAmB,EAAnB;MAAA;IAAA;;sBACF,MAA0D,CAA1DO,YAAA,CAA0DqB,0BAAA;MAAzCC,IAAI,EAAC,MAAM;MAACC,KAAK,EAAC;QACnCvB,YAAA,CAIkBqB,0BAAA;MAJDE,KAAK,EAAC;IAAI;MACdC,OAAO,EAAAC,QAAA,CAAEC,KAAK,KACvB1B,YAAA,CAAkH2B,mBAAA;QAAxGlC,KAAmC,EAAnC;UAAA;UAAA;QAAA