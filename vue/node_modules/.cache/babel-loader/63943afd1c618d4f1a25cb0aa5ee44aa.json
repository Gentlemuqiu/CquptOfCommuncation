=== activeArea}\" @click=\"loadArea(1, item)\"\n                     v-for=\"item in areas\" :key=\"item\">{{ item }}</el-button>\n        </div>\n\n        <!-- 根据地区加载的菜品列表 -->\n        <div style=\"margin: 10px 0;\">\n          <el-row :gutter=\"10\" v-if=\"tableDataArea.length > 0\">\n            <el-col :span=\"4\" v-for=\"item in tableDataArea\" :key=\"item.id\">\n              <el-card style=\"margin-bottom: 10px; cursor: pointer\" @click=\"$router.push('/front/detail?id=' + item.id)\">\n                <div><el-image :src=\"item.img\" style=\"width: 100%; height: 150px\"></el-image></div>\n                <div style=\"text-align: center; font-size: 12px; white-space: nowrap; overflow: hidden; text-overflow: ellipsis;\">\n                  {{ item.name }}\n                </div>\n              </el-card>\n            </el-col>\n          </el-row>\n          <div v-else style=\"padding: 20px; color: #666; min-height: 150px\">\n            暂无数据...\n          </div>\n\n          <!-- 分页 -->\n          <div style=\"margin: 10px 0\">\n            <el-pagination\n                @size-change=\"handleSizeChange\"\n                @current-change=\"handleCurrentChange\"\n                :current-page=\"pageNum\"\n                :page-sizes=\"[5, 10, 20]\"\n                :page-size=\"pageSize\"\n                layout=\"prev, pager, next\"\n                :total=\"total\">\n            </el-pagination>\n          </div>\n        </div>\n\n      </el-col>\n    </el-row>\n  </div>\n</template>\n\n<script>\nimport request from \"@/utils/request\";\n\nexport default {\n  name: \"Home\",\n  data() {\n    return {\n      // 图片和链接的对象数组\n      imgs: [\n        { img: require(\"@/assets/image/bg2.png\"), link: 'https://www.cqupt.edu.cn/' },\n        { img: require(\"@/assets/image/bg1.png\"), link: 'https://software.cqupt.edu.cn/' },\n        { img: require(\"@/assets/image/bg3.png\"), link: 'https://zjc.cqupt.edu.cn/' }\n      ],\n      tableDataArea: [],\n      tableDataHot: [\n      {\n        id: 1,\n        img: 'http://ali-img.kaiyanapp.com/40036ba884aed65c07a5ed1525e6cb2a.jpeg?imageMogr2/quality/100',\n        name: '蒸鹅'\n      },\n      {\n        id: 2,\n        img: 'https://example.com/images/dish2.jpg',\n        name: '宫保鸡丁'\n      },\n      {\n        id: 3,\n        img: 'https://example.com/images/dish3.jpg',\n        name: '麻辣火锅'\n      },\n      {\n        id: 4,\n        img: 'https://example.com/images/dish3.jpg',\n        name: '麻辣火锅'\n      },\n      {\n        id: 5,\n        img: 'https://example.com/images/dish3.jpg',\n        name: '麻辣火锅'\n      },\n      {\n        id: 6,\n        img: 'https://example.com/images/dish3.jpg',\n        name: '麻辣火锅'\n      }],\n      areas: ['莘莘食堂', '中心食堂', '千禧鹤', '滨湖餐厅', '大西北食堂'],\n      pageNum: 1,\n      pageSize: 8,\n      total: 0,\n      activeArea: ''\n    };\n  },\n  created() {\n    this.user = sessionStorage.getItem(\"user\") ? JSON.parse(sessionStorage.getItem(\"user\")) : {};\n    this.loadData();\n  },\n  methods: {\n    // 获取图片链接\n    getLinkForItem(index) {\n      return this.imgs[index].link;\n    },\n\n    // 加载地区数据\n    loadArea(pageNum, area) {\n      this.activeArea = area;\n      request.get(\"/movie/page/area\", {\n        params: { pageNum, pageSize: this.pageSize, area }\n      }).then(res => {\n        this.tableDataArea = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    // 加载推荐数据\n    loadData() {\n      this.loadScore();\n      this.loadHot();\n      this.loadDate();\n      this.loadArea(1, this.areas[0]); // 默认加载第一个区域\n    },\n\n    loadScore() {\n      request.get(\"/movie/type/score\").then(res => {\n        this.tableDataScore = res.data;\n      });\n    },\n\n    loadHot() {\n      request.get(\"/movie/type/hot\").then(res => {\n        this.tableDataHot = res.data;\n      });\n    },\n\n    loadDate() {\n      request.get(\"/movie/type/date\").then(res => {\n        this.tableDataDate = res.data;\n      });\n    },\n\n    // 改变页面大小时触发\n    handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n      this.loadArea(1, this.activeArea);\n    },\n\n    // 改变当前页码时触发\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.loadArea(pageNum, this.activeArea);\n    }\n  }\n};\n</script>\n\n<style scoped>\n.active {\n  color: white;\n  background-color: dodgerblue;\n}\n</style>\n"],"mappings":";;;EACOA,KAAsB,EAAtB;IAAA;EAAA;AAAsB;;;;EAuBRA,KAA4G,EAA5G;IAAA;IAAA;IAAA;IAAA;IAAA;EAAA;AAA4G;;EASpHA,KAA8D,EAA9D;IAAA;IAAA;IAAA;EAAA;AAA8D;;EAM9DA,KAAuB,EAAvB;IAAA;EAAA;AAAuB;;EAKfA,KAA4G,EAA5G;IAAA;IAAA;IAAA;IAAA;IAAA;EAAA;AAA4G;;;EAM3GA,KAAqD,EAArD;IAAA;IAAA;IAAA;EAAA;;;EAKPA,KAAsB,EAAtB;IAAA;EAAA;AAAsB;;;;;;;;;;uBAtDnCC,mBAAA,CAqEM,OArENC,UAqEM,GApEJC,YAAA,CAmESC,iBAAA;sBAlEP,MAiES,CAjETD,YAAA,CAiESE,iBAAA;wBAhEP,MAAY,CAAZC,mBAAA,SAAY,EACZH,YAAA,CAMcI,sBAAA;QANDC,MAAM,EAAC;MAAO;0BACP,MAA6B,E,kBAA/CP,mBAAA,CAImBQ,SAAA,QAAAC,WAAA,CAJuBC,KAAA,CAAAC,IAAI,GAApBC,IAAI,EAAEC,KAAK;+BAArCC,YAAA,CAImBC,2BAAA;YAJ8BC,GAAG,EAAEJ,IAAI,CAACK;;8BACzD,MAEI,CAFJC,mBAAA,CAEI;cAFAC,IAAI,EAAEC,QAAA,CAAAC,cAAc,CAACR,KAAK;cAAGS,MAAM,EAAC;gBACtCJ,mBAAA,CAAiD;cAA3CK,GAAG,EAAEX,IAAI,CAACK,GAAG;cAAEO,GAAG,EAAC,EAAE;cAACC,KAAK,EAAC;;;;;;UAKxCpB,mBAAA,UAAa,E,0BACba,mBAAA,CAEM;QAFDnB,KAA4D,EAA5D;UAAA;UAAA;UAAA;QAAA;MAA4D,IAC/DmB,mBAAA,CAAiE;QAA5DnB,KAA0C,EAA1C;UAAA;UAAA;QAAA;MAA0C,IAACmB,mBAAA,CAAW,WAAR,MAAI,E,wBAGzDb,mBAAA,UAAa,EACba,mBAAA,CAWM,cAVJhB,YAAA,CASSC,iBAAA;QATAuB,MAAM,EAAE;MAAE;0BACC,MAA4B,E,kBAA9C1B,mBAAA,CAOSQ,SAAA,QAAAC,WAAA,CAPwBC,KAAA,CAAAiB,YAAY,EAApBf,IAAI;+BAA7BE,YAAA,CAOSV,iBAAA;YAPAwB,IAAI,EAAE,CAAC;YAAgCZ,GAAG,EAAEJ,IAAI,CAACiB;;8BACxD,MAKU,CALV3B,YAAA,CAKU4B,kBAAA;cALD/B,KAA4C,EAA5C;gBAAA;gBAAA;cAAA,CAA4C;cAAEgC,OAAK,EAAAC,MAAA,IAAEC,IAAA,CAAAC,OAAO,CAACC,IAAI,uBAAuBvB,IAAI,CAACiB,EAAE;;gCACtG,MAAmF,CAAnFX,mBAAA,CAAmF,cAA9EhB,YAAA,CAAwEkC,mBAAA;gBAA7Db,GAAG,EAAEX,IAAI,CAACK,GAAG;gBAAElB,KAAkC,EAAlC;kBAAA;kBAAA;gBAAA;kDAC/BmB,mBAAA,CAEM,OAFNmB,UAEM,EAAAC,gBAAA,CADD