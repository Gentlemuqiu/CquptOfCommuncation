=\"address\" label=\"地址\"></el-table-column>\n      <el-table-column label=\"禁止评论\">\n        <template #default=\"scope\">\n          <el-switch\n              @change=\"change(scope.row)\"\n              v-model=\"scope.row.forbidComment\"\n              active-color=\"#f00\"\n              inactive-color=\"#ddd\">\n          </el-switch>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"禁止登录\">\n        <template #default=\"scope\">\n          <el-switch\n              @change=\"change(scope.row)\"\n              v-model=\"scope.row.forbidLogin\"\n              active-color=\"#f00\"\n              inactive-color=\"#ddd\">\n          </el-switch>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"角色\">\n        <template #default=\"scope\">\n          <span v-if=\"scope.row.role === 1\">管理员</span>\n          <span v-if=\"scope.row.role === 2\">普通用户</span>\n          <span v-if=\"scope.row.role === 3\">食堂管理员</span>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"操作\">\n        <template #default=\"scope\">\n          <el-button size=\"mini\" @click=\"handleEdit(scope.row)\">编辑</el-button>\n          <el-popconfirm title=\"确定删除吗？\" @confirm=\"handleDelete(scope.row.id)\">\n            <template #reference>\n              <el-button size=\"mini\" type=\"danger\">删除</el-button>\n            </template>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <div style=\"margin: 10px 0\">\n      <el-pagination\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n          :current-page=\"pageNum\"\n          :page-sizes=\"[5, 10, 20]\"\n          :page-size=\"pageSize\"\n          layout=\"total, sizes, prev, pager, next, jumper\"\n          :total=\"total\">\n      </el-pagination>\n    </div>\n\n    <el-dialog title=\"信息\" v-model=\"dialogVisible\" width=\"30%\">\n      <el-form :model=\"entity\" label-width=\"120px\">\n        <el-form-item label=\"用户名\">\n          <el-input v-model=\"entity.username\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"昵称\">\n          <el-input v-model=\"entity.nickName\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"手机号\">\n          <el-input v-model=\"entity.phone\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"年龄\">\n          <el-input v-model=\"entity.age\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"性别\">\n          <el-radio v-model=\"entity.sex\" label=\"男\">男</el-radio>\n          <el-radio v-model=\"entity.sex\" label=\"女\">女</el-radio>\n          <el-radio v-model=\"entity.sex\" label=\"未知\">未知</el-radio>\n        </el-form-item>\n        <el-form-item label=\"角色身份\">\n          <el-radio v-model=\"entity.role\" label=\"3\">食堂管理员</el-radio>\n          <el-radio v-model=\"entity.role\" label=\"2\">普通用户</el-radio>\n\n\n        </el-form-item>\n        <el-form-item label=\"地址\">\n          <el-input type=\"textarea\" v-model=\"entity.address\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n      </el-form>\n      <template #footer>\n          <span class=\"dialog-footer\">\n            <el-button @click=\"dialogVisible = false\">取 消</el-button>\n            <el-button type=\"primary\" @click=\"save\">确 定</el-button>\n          </span>\n      </template>\n    </el-dialog>\n\n  </div>\n</template>\n\n<script>\n\nimport request from \"@/utils/request\";\nlet url = \"/user\"\n\nexport default {\n  name: 'User',\n  components: {\n\n  },\n  data() {\n    return {\n      loading: true,\n      entity: {},\n      dialogVisible: false,\n      search: '',\n      pageNum: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: []\n    }\n  },\n  created() {\n    this.load()\n  },\n  methods: {\n    change(row) {\n      this.entity = JSON.parse(JSON.stringify(row))\n      request.put(url, this.entity).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"操作成功\"\n          })\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          })\n        }\n        this.load() // 刷新表格的数据\n      })\n    },\n    load() {\n      this.loading = true\n      request.get(url + \"/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        this.loading = false\n        this.tableData = res.data.records\n        this.total = res.data.total\n      })\n    },\n    add() {\n      this.dialogVisible = true\n      this.entity = {}\n    },\n    save() {\n      if (this.entity.id) {  // 更新\n        request.put(url, this.entity).then(res => {\n          console.log(res)\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            })\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            })\n          }\n          this.load() // 刷新表格的数据\n          this.dialogVisible = false  // 关闭弹窗\n        })\n      }  else {  // 新增\n        request.post(url, this.entity).then(res => {\n          console.log(res)\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            })\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            })\n          }\n\n          this.load() // 刷新表格的数据\n          this.dialogVisible = false  // 关闭弹窗\n        })\n      }\n\n    },\n    handleEdit(row) {\n      this.entity = JSON.parse(JSON.stringify(row))\n      this.dialogVisible = true\n    },\n    handleDelete(id) {\n      console.log(id)\n      request.delete(url + \"/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          })\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          })\n        }\n        this.load()  // 删除之后重新加载表格的数据\n      })\n    },\n    handleSizeChange(pageSize) {   // 改变当前每页的个数触发\n      this.pageSize = pageSize\n      this.load()\n    },\n    handleCurrentChange(pageNum) {  // 改变当前页码触发\n      this.pageNum = pageNum\n      this.load()\n    }\n  }\n}\n</script>\n"],"mappings":";;EACOA,KAAqB,EAArB;IAAA;EAAA;AAAqB;;EAGnBA,KAAsB,EAAtB;IAAA;EAAA;AAAsB;;EAKtBA,KAAsB,EAAtB;IAAA;EAAA;AAAsB;;;;;;;;;;;EAwDtBA,KAAsB,EAAtB;IAAA;EAAA;AAAsB;;EA0CfC,KAAK,EAAC;AAAe;;;;;;;;;;;;;;uBA1GnCC,mBAAA,CAiHM,OAjHNC,UAiHM,GA9GJC,mBAAA,CAEM,OAFNC,UAEM,GADJC,YAAA,CAAqDC,oBAAA;IAA1CC,IAAI,EAAC,SAAS;IAAEC,OAAK,EAAEC,QAAA,CAAAC;;sBAAK,MAAEC,MAAA,SAAAA,MAAA,Q,iBAAF,IAAE,E;;wBAI3CR,mBAAA,CAGM,OAHNS,UAGM,GAFJP,YAAA,CAAwFQ,mBAAA;gBAArEC,KAAA,CAAAC,MAAM;+DAAND,KAAA,CAAAC,MAAM,GAAAC,MAAA;IAAEC,WAAW,EAAC,QAAQ;IAAClB,KAAkB,EAAlB;MAAA;IAAA,CAAkB;IAACmB,SAAS,EAAT;+BACnEb,YAAA,CAA+EC,oBAAA;IAApEC,IAAI,EAAC,SAAS;IAACR,KAAwB,EAAxB;MAAA;IAAA,CAAwB;IAAES,OAAK,EAAEC,QAAA,CAAAU;;sBAAM,MAAER,MAAA,SAAAA,MAAA,Q,iBAAF,IAAE,E;;uDAErES,YAAA,CAkDWC,mBAAA;IAhDNC,IAAI,EAAER,KAAA,CAAAS,SAAS;IAChBC,MAAM,EAAN,EAAM;IACNC,MAAM,EAAN,EAAM;IACN1B,KAAmB,EAAnB;MAAA;IAAA;;sBACF,MAAiE,CAAjEM,YAAA,CAAiEqB,0BAAA;MAAhDC,IAAI,EAAC,IAAI;MAACC,KAAK,EAAC,IAAI;MAACC,QAAQ,EAAR;QACtCxB,YAAA,CAA+DqB,0BAAA;MAA9CC,IAAI,EAAC,UAAU;MAACC,KAAK,EAAC;QACvCvB,YAAA,CAA8DqB,0BAAA;MAA7CC,IAAI,EAAC,UAAU;MAACC,KAAK,EAAC;QACvCvB,YAAA,CAA4DqB,0BAAA;MAA3CC,IAAI,EAAC,OAAO;MAACC,KAAK,EAAC;QACpCvB,YAAA,CAAyDqB,0BAAA;MAAxCC,IAAI,EAAC,KAAK;MAACC,KAAK,EAAC;QAClCvB,YAAA,CAAyDqB,0BAAA;MAAxCC,IAAI,EAAC,KAAK;MAACC,KAAK,EAAC;QAClCvB,YAAA,CAA6DqB,0BAAA;MAA5CC,IAAI,EAAC,SAAS;MAACC,KAAK,EAAC;QACtCvB,YAAA,CASkBqB,0