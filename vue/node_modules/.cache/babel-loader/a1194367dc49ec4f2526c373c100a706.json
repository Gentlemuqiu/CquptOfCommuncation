训二\\javaweb-main\\vue\\src\\views\\Comment.vue"],"sourcesContent":["<template>\n  <div style=\"padding: 10px\">\n\n<!--    功能区域-->\n\n\n<!--    搜索区域-->\n    <div style=\"margin: 10px 0\">\n      <el-input v-model=\"search\" placeholder=\"请输入关键字\" style=\"width: 20%\" clearable></el-input>\n      <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"load\">查询</el-button>\n    </div>\n    <el-table\n        v-loading=\"loading\"\n        :data=\"tableData\"\n        border\n        stripe\n        style=\"width: 100%\">\n      <el-table-column prop=\"title\" label=\"标题\"></el-table-column>\n      <el-table-column label=\"内容\">\n        <template #default=\"scope\">\n          <div style=\" text-overflow:ellipsis;width:100%;\"> {{ scope.row.content.length > 100 ?\n              scope.row.content.substring(0, 100) + '...' : scope.row.content}}</div>\n        </template>\n      </el-table-column>\n      <el-table-column prop=\"username\" label=\"评论人\"></el-table-column>\n      <el-table-column prop=\"time\" label=\"评论时间\"></el-table-column>\n<!--      <el-table-column prop=\"parentId\" label=\"父ID\"></el-table-column>-->\n      <!--      <el-table-column prop=\"foreignId\" label=\"关联id\"></el-table-column>-->\n      <!--      <el-table-column prop=\"type\" label=\"类型\"></el-table-column>-->\n\n      <el-table-column label=\"操作\">\n        <template #default=\"scope\">\n<!--          <el-button size=\"mini\" @click=\"handleEdit(scope.row)\">编辑</el-button>-->\n          <el-popconfirm title=\"确定删除吗？\" @confirm=\"handleDelete(scope.row.id)\">\n            <template #reference>\n              <el-button size=\"mini\" type=\"danger\">删除</el-button>\n            </template>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <div style=\"margin: 10px 0\">\n      <el-pagination\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n          :current-page=\"pageNum\"\n          :page-sizes=\"[5, 10, 20]\"\n          :page-size=\"pageSize\"\n          layout=\"total, sizes, prev, pager, next, jumper\"\n          :total=\"total\">\n      </el-pagination>\n    </div>\n\n    <el-dialog title=\"信息\" v-model=\"dialogVisible\" width=\"30%\">\n    <el-form :model=\"entity\" label-width=\"120px\">\n        <el-form-item label=\"内容\">\n          <el-input v-model=\"entity.content\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"评论人\">\n          <el-input v-model=\"entity.username\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"评论时间\">\n          <el-date-picker style=\"width: 80%\" v-model=\"entity.time\" type=\"datetime\" value-format=\"YYYY-MM-DD HH:mm:ss\" placeholder=\"选择日期时间\"></el-date-picker>\n        </el-form-item>\n        <el-form-item label=\"父ID\">\n          <el-input v-model=\"entity.parentId\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"关联id\">\n          <el-input v-model=\"entity.foreignId\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"类型\">\n          <el-input v-model=\"entity.type\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"标题\">\n          <el-input v-model=\"entity.title\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n\n    </el-form>\n      <template #footer>\n          <span class=\"dialog-footer\">\n            <el-button @click=\"dialogVisible = false\">取 消</el-button>\n            <el-button type=\"primary\" @click=\"save\">确 定</el-button>\n          </span>\n      </template>\n    </el-dialog>\n\n  </div>\n</template>\n\n<script>\n\nimport request from \"@/utils/request\";\nlet url = \"/message\"\n\nexport default {\n  name: 'Message',\n  components: {\n\n  },\n  data() {\n    return {\n      loading: true,\n      entity: {},\n      dialogVisible: false,\n      search: '',\n      pageNum: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: []\n    }\n  },\n  created() {\n    this.load()\n  },\n  methods: {\n    load() {\n      this.loading = true\n      request.get(url + \"/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        this.loading = false\n        this.tableData = res.data.records\n        this.total = res.data.total\n      })\n    },\n    add() {\n      this.dialogVisible = true\n      this.entity = {}\n    },\n    save() {\n      if (this.entity.id) {  // 更新\n        request.put(url, this.entity).then(res => {\n          console.log(res)\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            })\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            })\n          }\n          this.load() // 刷新表格的数据\n          this.dialogVisible = false  // 关闭弹窗\n        })\n      }  else {  // 新增\n        request.post(url, this.entity).then(res => {\n          console.log(res)\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            })\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            })\n          }\n\n          this.load() // 刷新表格的数据\n          this.dialogVisible = false  // 关闭弹窗\n        })\n      }\n\n    },\n    handleEdit(row) {\n      this.entity = JSON.parse(JSON.stringify(row))\n      this.dialogVisible = true\n    },\n    handleDelete(id) {\n      request.delete(url + \"/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          })\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          })\n        }\n        this.load()  // 删除之后重新加载表格的数据\n      })\n    },\n    handleSizeChange(pageSize) {   // 改变当前每页的个数触发\n      this.pageSize = pageSize\n      this.load()\n    },\n    handleCurrentChange(pageNum) {  // 改变当前页码触发\n      this.pageNum = pageNum\n      this.load()\n    }\n  }\n}\n</script>\n"],"mappings":"AA4FA,OAAOA,OAAM,MAAO,iBAAiB;AACrC,IAAIC,GAAE,GAAI,UAAS;AAEnB,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,UAAU,EAAE,CAEZ,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,IAAI;MACbC,MAAM,EAAE,CAAC,CAAC;MACVC,aAAa,EAAE,KAAK;MACpBC,MAAM,EAAE,EAAE;MACVC,OAAO,EAAE,CAAC;MACVC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,CAAC;MACRC,SAAS,EAAE;IACb;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,IAAI,CAAC;EACZ,CAAC;EACDC,OAAO,EAAE;IACPD,IAAIA,CAAA,EAAG;MACL,IAAI,CAACT,OAAM,GAAI,IAAG;MAClBL,OAAO,CAACgB,GAAG,CAACf,GAAE,GAAI,OAAO,EAAE;QACzBgB,MAAM,EAAE;UACNR,OAAO,EAAE,IAAI,CAACA,OAAO;UACrBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBF,MAAM,EAAE,IAAI,CAACA;QACf;MACF,CAAC,CAAC,CAACU,IAAI,CAACC,GAAE,IAAK;QACb,IAAI,CAACd,OAAM,GAAI,KAAI;QACnB,IAAI,CAACO,SAAQ,GAAIO,GAAG,CAACf,IAAI,CAACgB,OAAM;QAChC,IAAI,CAACT,KAAI,GAAIQ,GAAG,CAACf,IAAI,CAACO,KAAI;MAC5B,CAAC;IACH,CAAC;IACDU,GAAGA,CAAA,EAAG;MACJ,IAAI,CAACd,aAAY,GAAI,IAAG;MACxB,IAAI,CAACD,MAAK,GAAI,CAAC;IACjB,CAAC;IACDgB,IAAIA,CAAA,EAAG;MACL,IAAI,IAAI,CAAChB,MAAM,CAACiB,EAAE,EAAE;QAAG;QACrBvB,OAAO,CAACwB,GAAG,CAACvB,GAAG,EAAE,IAAI,CAACK,MAAM,CAAC,CAACY,IAAI,CAACC,GAAE,IAAK;UACxCM,OAAO,CAACC,GAAG,CAACP,GAAG;UACf,IAAIA,GAAG,CAACQ,IAAG,KAAM,GAAG,EAAE;YACpB,IAAI,CAACC,QAAQ,CAAC;cACZC,IAAI,EAAE,SAAS;cACfC,OAAO,EAAE;YACX,CAAC;UACH,OAAO;YACL,IAAI,CAACF,QAAQ,CAAC;cACZC,IAAI,EAAE,OAAO;cACbC,OAAO,EAAEX,GAAG,CAACY;YACf,CAAC;UACH;UACA,IAAI,CAACjB,IAAI,CAAC,GAAE;UACZ,IAAI,CAACP,aAAY,GAAI,KAAI,EAAG;QAC9B,CAAC;MACH,OAAQ;QAAG;QACTP,OAAO,CAACgC,IAAI,CAAC/B,GAAG,EAAE,IAAI,CAACK,MAAM,CAAC,CAACY,IAAI,CAACC,GAAE,IAAK;U