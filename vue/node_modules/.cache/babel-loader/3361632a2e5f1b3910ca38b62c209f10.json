t","clearable","multiple","tagsArr","placeholder","onChange","changeTags","_Fragment","_renderList","item","_component_el_option","key","value","_createElementVNode","_hoisted_3","_component_el_button","type","onClick","update","loading","_cache","_component_el_image","networkImage"],"sources":["C:\\Users\\16278\\Desktop\\作业\\软件开发实训二\\javaweb-main\\vue\\src\\views\\Person.vue"],"sourcesContent":["<template>\n  <div>\n    <!-- 用户信息卡片 -->\n    <el-card style=\"width: 40%; margin: 10px; float: left;\">\n      <el-form ref=\"form\" :model=\"form\" label-width=\"80px\">\n        <!-- 上传头像 -->\n        <el-form-item>\n          <el-upload\n            class=\"avatar-uploader\"\n            action=\"http://localhost:8080/files/upload\"\n            :show-file-list=\"false\"\n            :on-success=\"handleAvatarSuccess\"\n          >\n            <img v-if=\"form.avatar\" :src=\"form.avatar\" class=\"avatar\" alt=\"用户头像\">\n            <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\n          </el-upload>\n        </el-form-item>\n        <!-- 用户信息表单 -->\n        <el-form-item label=\"用户名\">\n          <el-input v-model=\"form.username\" disabled></el-input>\n        </el-form-item>\n        <el-form-item label=\"昵称\">\n          <el-input v-model=\"form.nickName\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"手机号\">\n          <el-input v-model=\"form.phone\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"年龄\">\n          <el-input v-model=\"form.age\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"性别\">\n          <el-input v-model=\"form.sex\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"地址\">\n          <el-input v-model=\"form.address\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"标签\">\n          <el-select\n            clearable\n            multiple\n            v-model=\"form.tagsArr\"\n            placeholder=\"请选择\"\n            style=\"width: 100%\"\n            @change=\"changeTags\"\n          >\n            <el-option\n              v-for=\"item in ['喜剧', '悬疑', '爱情', '动作', '动画', '犯罪', '惊悚', '冒险', '历史', '战争', '纪录片', '科幻']\"\n              :key=\"item\"\n              :label=\"item\"\n              :value=\"item\"\n            ></el-option>\n          </el-select>\n        </el-form-item>\n      </el-form>\n      <!-- 保存按钮 -->\n      <div style=\"text-align: center\">\n        <el-button type=\"primary\" @click=\"update\" :loading=\"loading\">保存</el-button>\n      </div>\n    </el-card>\n\n    <!-- 用户网络信息展示 -->\n    <el-card style=\"width: 55%; margin: 10px; float: right;\">\n      <div style=\"padding: 10px; background-color: #f5f5f5; margin-bottom: 10px;\">\n        <h3>用户网络配置信息</h3>\n      </div>\n      <div>\n        <el-image :src=\"networkImage\" style=\"width: 100%; height: auto;\" alt=\"网络信息图片\"></el-image>\n      </div>\n    </el-card>\n  </div>\n</template>\n\n<script>\nimport request from \"@/utils/request\";\n\nexport default {\n  name: \"Person\",\n  data() {\n    return {\n      form: {},\n      loading: false, // 加载状态\n      // 网络配置信息图片路径\n      networkImage: require(\"@/assets/css/404.svg\"), // 将你的图片路径替换为实际路径\n    };\n  },\n  created() {\n    let str = sessionStorage.getItem(\"user\") || \"{}\";\n    this.form = JSON.parse(str);\n    this.form.tagsArr = this.form.tags ? this.form.tags.split(\",\").map(tag => tag.trim()) : [];\n  },\n  methods: {\n    handleAvatarSuccess(res) {\n      // 上传成功后更新头像路径\n      this.form.avatar = res.data.avatarUrl; // 假设返回数据为 avatarUrl\n    },\n    changeTags() {\n      if (this.form.tagsArr.length > 3) {\n        this.$message.error(\"最多选择3个个性标签\");\n        this.form.tagsArr = this.form.tagsArr.slice(0, 3); // 使用 slice 保证不修改原数组\n      }\n    },\n    update() {\n      this.loading = true; // 开始加载\n      // 保存标签为字符串形式\n      if (this.form.tagsArr) {\n        this.form.tags = this.form.tagsArr.join(\", \");\n      }\n      request.put(\"/user\", this.form).then((res) => {\n        this.loading = false; // 结束加载\n        if (res.code === \"0\") {\n          this.$message({\n            type: \"success\",\n            message: \"更新成功\",\n          });\n          sessionStorage.setItem(\"user\", JSON.stringify(this.form)); // 更新 sessionStorage\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg,\n          });\n        }\n      }).catch(() => {\n        this.loading = false; // 请求出错时停止加载\n        this.$message({\n          type: \"error\",\n          message: \"更新失败，请稍后再试\",\n        });\n      });\n    },\n  },\n};\n</script>\n\n<style scoped>\n.avatar-uploader .el-upload {\n  border: 1px dashed #d9d9d9;\n  border-radius: 6px;\n  cursor: pointer;\n  position: relative;\n  overflow: hidden;\n}\n.avatar-uploader .el-upload:hover {\n  border-color: #409EFF;\n}\n.avatar-uploader-icon {\n  font-size: 28px;\n  color: #8c939d;\n  width: 178px;\n  height: 178px;\n  line-height: 178px;\n  text-align: center;\n}\n.avatar {\n  width: 178px;\n  height: 178px;\n  display: block;\n  border-radius: 50%;\n}\n</style>\n"],"mappings":";;;;EAcsBA,KAAK,EAAC;;;EAyCjBC,KAA0B,EAA1B;IAAA;EAAA;AAA0B;;;;;;;;;;;uBAtDnCC,mBAAA,CAoEM,cAnEJC,mBAAA,YAAe,EACfC,YAAA,CAuDUC,kBAAA;IAvDDJ,KAA8C,EAA9C;MAAA;MAAA;MAAA;IAAA;EAA8C;sBACrD,MAiDU,CAjDVG,YAAA,CAiDUE,kBAAA;MAjDDC,GAAG,EAAC,MAAM;MAAEC,KAAK,EAAEC,KAAA,CAAAC,IAAI;MAAE,aAAW,EAAC;;wBAC5C,MAAa,CAAbP,mBAAA,UAAa,EACbC,YAAA,CAUeO,uBAAA;0BATb,MAQY,CARZP,YAAA,CAQYQ,oBAAA;UAPVZ,KAAK,EAAC,iBAAiB;UACvBa,MAAM,EAAC,oCAAoC;UAC1C,gBAAc,EAAE,KAAK;UACrB,YAAU,EAAEC,QAAA,CAAAC;;4BAEb,MAAqE,CAA1DN,KAAA,CAAAC,IAAI,CAACM,MAAM,I,cAAtBd,mBAAA,CAAqE;;YAA5Ce,GAAG,EAAER,KAAA,CAAAC,IAAI,CAACM,MAAM;YAAEhB,KAAK,EAAC,QAAQ;YAACkB,GAAG,EAAC;gEAC9DhB,mBAAA,CAAwD,KAAxDiB,UAAwD,G;;;;UAG5DhB,mBAAA,YAAe,EACfC,YAAA,CAEeO,uBAAA;QAFDS,KAAK,EAAC;MAAK;0BACvB,MAAsD,CAAtDhB,YAAA,CAAsDiB,mBAAA;sBAAnCZ,KAAA,CAAAC,IAAI,CAACY,QAAQ;qEAAbb,KAAA,CAAAC,IAAI,CAACY,QAAQ,GAAAC,MAAA;UAAEC,QAAQ,EAAR;;;UAEp