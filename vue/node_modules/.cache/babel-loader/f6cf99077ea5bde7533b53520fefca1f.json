         <el-button size=\"mini\" @click=\"handleEdit(scope.row)\">编辑</el-button>-->\n          <el-popconfirm title=\"确定删除吗？\" @confirm=\"handleDelete(scope.row.id)\">\n            <template #reference>\n              <el-button size=\"mini\" type=\"danger\">删除</el-button>\n            </template>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <div style=\"margin: 10px 0\">\n      <el-pagination\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n          :current-page=\"pageNum\"\n          :page-sizes=\"[5, 10, 20]\"\n          :page-size=\"pageSize\"\n          layout=\"total, sizes, prev, pager, next, jumper\"\n          :total=\"total\">\n      </el-pagination>\n    </div>\n\n    <el-dialog title=\"信息\" v-model=\"dialogVisible\" width=\"30%\">\n    <el-form :model=\"entity\" label-width=\"120px\">\n        <el-form-item label=\"内容\">\n          <el-input v-model=\"entity.content\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"评论人\">\n          <el-input v-model=\"entity.username\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"评论时间\">\n          <el-date-picker style=\"width: 80%\" v-model=\"entity.time\" type=\"datetime\" value-format=\"YYYY-MM-DD HH:mm:ss\" placeholder=\"选择日期时间\"></el-date-picker>\n        </el-form-item>\n        <el-form-item label=\"父ID\">\n          <el-input v-model=\"entity.parentId\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"关联id\">\n          <el-input v-model=\"entity.foreignId\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"类型\">\n          <el-input v-model=\"entity.type\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"标题\">\n          <el-input v-model=\"entity.title\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n\n    </el-form>\n      <template #footer>\n          <span class=\"dialog-footer\">\n            <el-button @click=\"dialogVisible = false\">取 消</el-button>\n            <el-button type=\"primary\" @click=\"save\">确 定</el-button>\n          </span>\n      </template>\n    </el-dialog>\n\n  </div>\n</template>\n\n<script>\n\nimport request from \"@/utils/request\";\nlet url = \"/message\"\n\nexport default {\n  name: 'Message',\n  components: {\n\n  },\n  data() {\n    return {\n      loading: true,\n      entity: {},\n      dialogVisible: false,\n      search: '',\n      pageNum: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: []\n    }\n  },\n  created() {\n    this.load()\n  },\n  methods: {\n    load() {\n      this.loading = true\n      request.get(url + \"/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        this.loading = false\n        this.tableData = res.data.records\n        this.total = res.data.total\n      })\n    },\n    add() {\n      this.dialogVisible = true\n      this.entity = {}\n    },\n    save() {\n      if (this.entity.id) {  // 更新\n        request.put(url, this.entity).then(res => {\n          console.log(res)\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            })\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            })\n          }\n          this.load() // 刷新表格的数据\n          this.dialogVisible = false  // 关闭弹窗\n        })\n      }  else {  // 新增\n        request.post(url, this.entity).then(res => {\n          console.log(res)\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            })\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            })\n          }\n\n          this.load() // 刷新表格的数据\n          this.dialogVisible = false  // 关闭弹窗\n        })\n      }\n\n    },\n    handleEdit(row) {\n      this.entity = JSON.parse(JSON.stringify(row))\n      this.dialogVisible = true\n    },\n    handleDelete(id) {\n      request.delete(url + \"/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          })\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          })\n        }\n        this.load()  // 删除之后重新加载表格的数据\n      })\n    },\n    handleSizeChange(pageSize) {   // 改变当前每页的个数触发\n      this.pageSize = pageSize\n      this.load()\n    },\n    handleCurrentChange(pageNum) {  // 改变当前页码触发\n      this.pageNum = pageNum\n      this.load()\n    }\n  }\n}\n</script>\n"],"mappings":";;EACOA,KAAqB,EAArB;IAAA;EAAA;AAAqB;;EAMnBA,KAAsB,EAAtB;IAAA;EAAA;AAAsB;;EAahBA,KAA2C,EAA3C;IAAA;IAAA;EAAA;AAA2C;;EAsBjDA,KAAsB,EAAtB;IAAA;EAAA;AAAsB;;EAsCfC,KAAK,EAAC;AAAe;;;;;;;;;;;;;uBA/EnCC,mBAAA,CAsFM,OAtFNC,UAsFM,GApFRC,mBAAA,YAAe,EAGfA,mBAAA,YAAe,EACXC,mBAAA,CAGM,OAHNC,UAGM,GAFJC,YAAA,CAAwFC,mBAAA;gBAArEC,KAAA,CAAAC,MAAM;+DAAND,KAAA,CAAAC,MAAM,GAAAC,MAAA;IAAEC,WAAW,EAAC,QAAQ;IAACZ,KAAkB,EAAlB;MAAA;IAAA,CAAkB;IAACa,SAAS,EAAT;2CACnEN,YAAA,CAA+EO,oBAAA;IAApEC,IAAI,EAAC,SAAS;IAACf,KAAwB,EAAxB;MAAA;IAAA,CAAwB;IAAEgB,OAAK,EAAEC,QAAA,CAAAC;;sBAAM,MAAEC,MAAA,SAAAA,MAAA,Q,iBAAF,IAAE,E;;mEAErEC,YAAA,CA6BWC,mBAAA;IA3BNC,IAAI,EAAEb,KAAA,CAAAc,SAAS;IAChBC,MAAM,EAAN,EAAM;IACNC,MAAM,EAAN,EAAM;IACNzB,KAAmB,EAAnB;MAAA;IAAA;;sBACF,MAA2D,CAA3DO,YAAA,CAA2DmB,0BAAA;MAA1CC,IAAI,EAAC,OAAO;MAACC,KAAK,EAAC;QACpCrB,YAAA,CAKkBmB,0BAAA;MALDE,KAAK,EAAC;IAAI;MACdC,OAAO,EAAAC,QAAA,CAAEC,KAAK,KACvB1B,mBAAA,CAC2E,OAD3E2B,UAC2E,EAAAC,