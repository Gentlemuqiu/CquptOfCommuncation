--    功能区域-->\n    <div style=\"margin: 10px 0\">\n      <el-button type=\"primary\" @click=\"add\">新增</el-button>\n    </div>\n\n<!--    搜索区域-->\n    <div style=\"margin: 10px 0\">\n      <el-input v-model=\"search\" placeholder=\"请输入关键字\" style=\"width: 20%\" clearable></el-input>\n      <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"load\">查询</el-button>\n    </div>\n    <el-table\n        v-loading=\"loading\"\n        :data=\"tableData\"\n        border\n        stripe\n        style=\"width: 100%\">\n      <el-table-column prop=\"name\" label=\"名称\"></el-table-column>\n      <el-table-column label=\"图片\">\n        <template #default=\"scope\">\n          <el-image style=\"width: 100px; height: 100px\" :src=\"scope.row.img\" :preview-src-list=\"[scope.row.img]\"></el-image>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"下载视频\">\n        <template #default=\"scope\">\n          <el-button v-if=\"scope.row.url\" type=\"primary\" @click=\"download(scope.row.url)\">下载</el-button>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"操作\" width=\"150\">\n        <template #default=\"scope\">\n          <el-button size=\"mini\" @click=\"handleEdit(scope.row)\">编辑</el-button>\n          <el-popconfirm title=\"确定删除吗？\" @confirm=\"handleDelete(scope.row.id)\">\n            <template #reference>\n              <el-button size=\"mini\" type=\"danger\">删除</el-button>\n            </template>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <div style=\"margin: 10px 0\">\n      <el-pagination\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n          :current-page=\"pageNum\"\n          :page-sizes=\"[5, 10, 20]\"\n          :page-size=\"pageSize\"\n          layout=\"total, sizes, prev, pager, next, jumper\"\n          :total=\"total\">\n      </el-pagination>\n    </div>\n\n    <el-dialog title=\"信息\" v-model=\"dialogVisible\" width=\"30%\">\n    <el-form :model=\"entity\" label-width=\"120px\">\n        <el-form-item label=\"名称\">\n          <el-input v-model=\"entity.name\" autocomplete=\"off\" style=\"width: 80%\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"图片\">\n          <el-upload action=\"http://localhost:9090/files/upload\" :on-success=\"fileSuccessUpload\" :file-list=\"fileList\">\n            <el-image style=\"width: 100px\" :src=\"entity.img\"></el-image>\n<!--            <el-button size=\"small\" type=\"primary\">点击上传</el-button>-->\n          </el-upload>\n        </el-form-item>\n      <el-form-item label=\"上传视频\">\n        <el-upload action=\"http://localhost:9090/files/upload\" :on-success=\"movieSuccessUpload\" :file-list=\"fileList\">\n          <el-button size=\"small\" type=\"primary\">点击上传</el-button>\n        </el-upload>\n      </el-form-item>\n\n    </el-form>\n      <template #footer>\n          <span class=\"dialog-footer\">\n            <el-button @click=\"dialogVisible = false\">取 消</el-button>\n            <el-button type=\"primary\" @click=\"save\">确 定</el-button>\n          </span>\n      </template>\n    </el-dialog>\n\n  </div>\n</template>\n\n<script>\n\nimport request from \"@/utils/request\";\nlet url = \"/movie\"\n\nexport default {\n  name: 'Movie',\n  components: {\n\n  },\n  data() {\n    return {\n      loading: true,\n      entity: {},\n      dialogVisible: false,\n      search: '',\n      pageNum: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: [],\n      fileList: []\n    }\n  },\n  created() {\n    this.load()\n  },\n  methods: {\n    download(url) {\n      window.open(url)\n    },\n    fileSuccessUpload(res) {\n      this.entity.img = res.data;\n    },\n    movieSuccessUpload(res) {\n      this.entity.url = res.data;\n    },\n    load() {\n      this.loading = true\n      request.get(url + \"/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        this.loading = false\n        this.tableData = res.data.records\n        this.total = res.data.total\n      })\n    },\n    add() {\n      this.dialogVisible = true\n      this.entity = {}\n      this.fileList = []\n    },\n    save() {\n      if (this.entity.id) {  // 更新\n        request.put(url, this.entity).then(res => {\n          console.log(res)\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            })\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            })\n          }\n          this.load() // 刷新表格的数据\n          this.dialogVisible = false  // 关闭弹窗\n        })\n      }  else {  // 新增\n        request.post(url, this.entity).then(res => {\n          console.log(res)\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            })\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            })\n          }\n\n          this.load() // 刷新表格的数据\n          this.dialogVisible = false  // 关闭弹窗\n        })\n      }\n\n    },\n    handleEdit(row) {\n      this.entity = JSON.parse(JSON.stringify(row))\n      this.dialogVisible = true\n      this.fileList = []\n    },\n    handleDelete(id) {\n      request.delete(url + \"/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          })\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          })\n        }\n        this.load()  // 删除之后重新加载表格的数据\n      })\n    },\n    handleSizeChange(pageSize) {   // 改变当前每页的个数触发\n      this.pageSize = pageSize\n      this.load()\n    },\n    handleCurrentChange(pageNum) {  // 改变当前页码触发\n      this.pageNum = pageNum\n      this.load()\n    }\n  }\n}\n</script>\n"],"mappings":"AAqFA,OAAOA,OAAM,MAAO,iBAAiB;AACrC,IAAIC,GAAE,GAAI,QAAO;AAEjB,eAAe;EACbC,IAAI,EAAE,OAAO;EACbC,UAAU,EAAE,CAEZ,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,IAAI;MACbC,MAAM,EAAE,CAAC,CAAC;MACVC,aAAa,EAAE,KAAK;MACpBC,MAAM,EAAE,EAAE;MACVC,OAAO,EAAE,CAAC;MACVC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,CAAC;MACRC,SAAS,EAAE,EAAE;MACbC,QAAQ,EAAE;IACZ;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,IAAI,CAAC;EACZ,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQA,CAAChB,GAAG,EAAE;MACZiB,MAAM,CAACC,IAAI,CAAClB,GAAG;IACjB,CAAC;IACDmB,iBAAiBA,CAACC,GAAG,EAAE;MACrB,IAAI,CAACf,MAAM,CAACgB,GAAE,GAAID,GAAG,CAACjB,IAAI;IAC5B,CAAC;IACDmB,kBAAkBA,CAACF,GAAG,EAAE;MACtB,IAAI,CAACf,MAAM,CAACL,GAAE,GAAIoB,GAAG,CAACjB,IAAI;IAC5B,CAAC;IACDW,IAAIA,CAAA,EAAG;MACL,IAAI,CAACV,OAAM,GAAI,IAAG;MAClBL,OAAO,CAACwB,GAAG,CAACvB,GAAE,GAAI,OAAO,EAAE;QACzBwB,MAAM,EAAE;UACNhB,OAAO,EAAE,IAAI,CAACA,OAAO;UACrBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBF,MAAM,EAAE,IAAI,CAACA;QACf;MACF,CAAC,CAAC,CAACkB,IAAI,CAACL,GAAE,IAAK;QACb,IAAI,CAAChB,OAAM,GAAI,KAAI;QACnB,IAAI,CAACO,SAAQ,GAAIS,GAAG,CAACjB,IAAI,CAACuB,OAAM;QAChC,IAAI,CAAChB,KAAI,GAAIU,GAAG,CAACjB,IAAI,CAACO,KAAI;MAC5B,CAAC;IACH,CAAC;IACDiB,GAAGA,CAAA,EAAG;MACJ,IAAI,CAACrB,aAAY,GAAI,IAAG;MACxB,IAAI,CAACD,MAAK,GAAI,CAAC;MACf,IAAI,CAACO,QAAO,GAAI,EAAC;IACnB,CAAC;IACDgB,IAAIA,CAAA,EAAG;MACL,IAAI,IAAI,CAACvB,MAAM,CAACwB,EAAE,EAAE;QAAG;QACrB9B,OAAO,CAAC+B,GAAG,CAAC9B,GAAG,EAAE,IAAI,CAACK,MAAM,CAAC,CAACoB,IAAI,CAACL,GAAE,IAAK;UACxCW,OAAO,CAACC,GAAG,CAACZ,GAAG;UACf,IAAIA,GAAG,CAACa,IAAG,KAAM,GAAG,EAAE;YACpB,IAAI,CAACC,QAAQ,CAAC;cACZC,IAAI,EAAE,SAAS;cACfC,OAAO,EAAE;YACX,CAAC;UACH,OAAO;YACL,IAAI,CAACF,QAAQ,CAAC;cACZC,IAAI,EAAE,OAAO;cACbC,OAAO,EAAEhB,GAAG,CAACiB;YACf,CAAC;UACH;UACA,IAAI,CAACvB,IAAI,CAAC,GAAE;UACZ,IAAI,CAACR,aAAY,GAAI,KAAI,EAAG;QAC9B,CAAC;MACH,OAAQ;QAAG;QACTP,OAAO,CAACuC,IAAI,CAACtC,GAAG,EAAE,IAAI,CAACK,MAAM,CAAC,CAACoB,IAAI,CAACL,GAAE,IAAK;UACzCW,OAAO,CAACC,GAAG