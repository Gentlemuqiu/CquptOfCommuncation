性\n      user: JSON.parse(sessionStorage.getItem(\"user\") || \"{}\"), // 当前用户信息\n    };\n  },\n  created() {\n    this.load(); // 初始化加载数据\n  },\n  methods: {\n    // 加载收藏数据\n    load() {\n      request.get(`/collect/user/${this.user.id}`).then((res) => {\n        if (res.code === \"0\") {\n          this.tableData = res.data; // 加载表格数据\n        } else {\n          this.$message.error(res.msg || \"加载失败\");\n        }\n      });\n    },\n    // 显示长评论内容\n    show(content) {\n      this.longComment = content;\n      this.vis = true;\n    },\n    // 删除收藏\n    handleDelete(id) {\n      request\n        .delete(`/collect/${id}`) // 调用后端的删除接口\n        .then((res) => {\n          if (res.code === \"0\") {\n            this.$message({\n              type: \"success\",\n              message: \"取消收藏成功\",\n            });\n            this.load(); // 刷新表格数据\n          } else {\n            this.$message.error(res.msg || \"取消收藏失败\");\n          }\n        })\n        .catch(() => {\n          this.$message.error(\"取消收藏失败，请稍后重试\");\n        });\n    },\n  },\n};\n</script>\n\n<style scoped>\n.el-table {\n  margin-top: 10px;\n}\n</style>\n"],"mappings":";;EACOA,KAAqB,EAArB;IAAA;EAAA;AAAqB;;;;;;;;;uBAA1BC,mBAAA,CA6BM,OA7BNC,UA6BM,GA5BJC,mBAAA,UAAa,EACbC,YAAA,CAoBWC,mBAAA;IApBAC,IAAI,EAAEC,KAAA,CAAAC,SAAS;IAAEC,MAAM,EAAN,EAAM;IAACC,MAAM,EAAN;;sBACjC,MAA0D,CAA1DN,YAAA,CAA0DO,0BAAA;MAAzCC,IAAI,EAAC,MAAM;MAACC,KAAK,EAAC;QACnCT,YAAA,CAIkBO,0BAAA;MAJDE,KAAK,EAAC,MAAM;MAACC,KAAK,EAAC,KAAK;MAACC,KAAK,EAAC;;MACnCC,OAAO,EAAAC,QAAA,CAAEC,KAAK,KACvBd,YAAA,CAAsFe,oBAAA;QAA3EC,IAAI,EAAC,SAAS;QAACC,IAAI,EAAC,OAAO;QAAEC,OAAK,EAAAC,MAAA,IAAEC,QAAA,CAAAC,IAAI,CAACP,KAAK,CAACQ,GAAG,CAACC,OAAO;;0BAAG,MAAEC,MAAA,QAAAA,MAAA,O,iBAAF,IAAE,E;;;;QAG9ExB,YAAA,CAA4DO,0BAAA;MAA3CC,IAAI,EAAC,MAAM;MAACC,KAAK,EAAC;QACnCT,YAAA,CAWkBO,0BAAA;MAXDE,KAAK,EAAC,IAAI;MAACC,KAAK,EAAC,KAAK;MAACC,KAAK,EAAC;;MACjCC,OAAO,EAAAC,QAAA,CAAEC,KAAK,KACvBd,YAAA,CAOgByB,wBAAA;QANdC,KAAK,EAAC,eAAe;QACpBC,SAAO,EAAAR,MAAA,IAAEC,QAAA,CA