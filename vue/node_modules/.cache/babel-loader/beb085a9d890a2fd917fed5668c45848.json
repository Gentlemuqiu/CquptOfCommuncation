assword: [\n          {required: true, message: '请输入密码', trigger: 'blur'},\n        ],\n      }\n    }\n  },\n  created() {\n    sessionStorage.removeItem(\"user\")\n  },\n  methods: {\n    login() {\n      this.$refs['form'].validate((valid) => {\n        if (valid) {\n          request.post(\"/user/login\", this.form).then(res => {\n            if (res.code === '0') {\n              this.$message({\n                type: \"success\",\n                message: \"登录成功\"\n              })\n              sessionStorage.setItem(\"user\", JSON.stringify(res.data))  // 缓存用户信息\n              if (res.data.role !== 1) {\n                this.$router.push(\"/front/home\")  //登录成功之后进行页面的跳转，跳转到主页\n              } else {\n                this.$router.push(\"/\")  //登录成功之后进行页面的跳转，跳转到主页\n              }\n\n            } else {\n              this.$message({\n                type: \"error\",\n                message: res.msg\n              })\n            }\n          })\n        }\n      })\n    }\n  }\n}\n</script>\n\n<style scoped>\n.bgImg {\n  background: url(\"../assets/bg.jpg\") no-repeat;\n  background-size: 60% 100vh;\n  background-color: LightYellow;\n\n}\n</style>\n"],"mappings":";;;EACOA,KAAoD,EAApD;IAAA;IAAA;IAAA;EAAA,CAAoD;EAACC,KAAK,EAAC;;;EACzDD,KAA0E,EAA1E;IAAA;IAAA;IAAA;IAAA;EAAA;AAA0E;;;;;;;uBADjFE,mBAAA,CAwBM,OAxBNC,UAwBM,GAvBJC,mBAAA,CAsBM,OAtBNC,UAsBM,G,4BArBJD,mBAAA,CAA2G;IAAtGJ,KAA+E,EAA/E;MAAA;MAAA;MAAA;MAAA;IAAA;EAA+E,GAAC,kBAAgB,QACrGM,YAAA,CAmBUC,kBAAA;IAnBDC,GAAG,EAAC,MAAM;IAAEC,KAAK,EAAEC,KAAA,CAAAC,IAAI;IAAEC,IAAI,EAAC,QAAQ;IAAEC,KAAK,EAAEH,KAAA,CAAAG;;sBACtD,MAEe,CAFfP,YAAA,CAEeQ,uBAAA;MAFDC,IAAI,EAAC;IAAU;wBAC3B,MAA8E,CAA9ET,YAAA,CAA8EU,mBAAA;QAApE,aAAW,EAAC,oBAAoB;oBAAUN,KAAA,CAAAC,IAAI,CAACM,QAAQ;mEAAbP,KAAA,CAAAC,IAAI,CAACM,QAAQ,GAAAC,MAAA;;;QAEnEZ,YAAA,CAEeQ,uBAAA;MAFDC,IAAI,EAAC;IAAU;wBAC3B,MAAsF,CAAtFT,YAAA,CAAsFU,mBAAA;QAA5E,aAAW,EAAC,cAAc;oBAAUN,KAAA,CAAAC,IAAI,CAACQ,QAAQ;mEAAbT,KAAA,CAAAC,IAAI,CAACQ,QAAQ,GAAAD,MAAA;QAAE,eAAa,EAAb;;;QAE/DZ,Y