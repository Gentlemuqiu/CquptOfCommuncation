e :src=\"item.img\" style=\"width: 100%; height: 150px\"></el-image></div>\n          <div style=\"text-align: center; font-size: 12px;white-space: nowrap;overflow: hidden;text-overflow: ellipsis;\">{{ item.name }}</div>\n        </el-card>\n      </el-col>\n    </el-row>\n    <div v-if=\"tableData.length === 0\" style=\"text-align: center; padding: 20px;\">\n      <el-empty description=\"暂无数据\"></el-empty>\n    </div>\n  </el-card>\n</template>\n\n<script>\nimport request from \"@/utils/request\";\n\nexport default {\n  name: \"Search\",\n  data() {\n    return {\n      tableData: []\n    }\n  },\n  created() {\n    this.fetchData();\n  },\n  watch: {\n    // 监听路由参数变化\n    '$route.query.name': {\n      handler: 'fetchData',\n      immediate: true\n    }\n  },\n  methods: {\n    fetchData() {\n      const name = this.$route.query.name || '';\n      request.get(\"/movie/search\", {\n        params: {\n          name: name\n        }\n      }).then(res => {\n        this.tableData = res.data || [];\n      }).catch(err => {\n        console.error('搜索失败：', err);\n        this.tableData = [];\n        this.$message.error('搜索失败，请稍后重试');\n      });\n    }\n  }\n}\n</script>"]}]}